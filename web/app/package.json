{
  "name": "squad-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "axios": "^0.21.1",
    "dateformat": "^5.0.3",
    "mdbreact": "^5.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga4": "^1.4.1",
    "react-helmet": "^6.1.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "node ./setup.js && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "importOrder": [
      "react",
      "react-dom",
      "^react(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
